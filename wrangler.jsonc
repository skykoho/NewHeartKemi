{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "heartkemy",
  "compatibility_date": "2026-01-18",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "heartkemy-production",
      "database_id": "local-heartkemy",
      "migrations_dir": "./migrations"
    }
  ]
}